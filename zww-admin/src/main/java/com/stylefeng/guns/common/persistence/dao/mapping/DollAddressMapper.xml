<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.common.persistence.dao.DollAddressMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.stylefeng.guns.common.persistence.model.DollAddress">
		<id column="id" property="id" />
		<result column="province" property="province" />
		<result column="city" property="city" />
		<result column="county" property="county" />
		<result column="street" property="street" />
		<result column="created_date" property="createdDate" />
		<result column="modified_date" property="modifiedDate" />
		<result column="default_flg" property="defaultFlg" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, province, city, county, street, created_date AS createdDate, modified_date AS modifiedDate, default_flg AS defaultFlg
    </sql>

</mapper>
